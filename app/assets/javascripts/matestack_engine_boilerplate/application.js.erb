window.process = { env: { NODE_ENV: '<%= Rails.env.to_s %>' } }

document.addEventListener('DOMContentLoaded', async () => {
 const { default: MatestackUiCore } = await System.import("matestack-ui-core");
 const Vue = MatestackUiCore.eventHub.constructor;

 new Vue({
   el: "#matestack-engine-app",
   store: MatestackUiCore.store
 })
})